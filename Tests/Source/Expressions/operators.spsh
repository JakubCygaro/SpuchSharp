import "STDLib";

fun main() {
    int a;
    a += 10;
    throwIf(a != 10);
    a -= 10;
    throwIf(a != 0);
    a = 1;
    a *= 10;
    throwIf(a != 10);
    a /= 10;
    throwIf(a != 1);
    a %= 1;
    throwIf(a != 0);

    int[] arr = [10];
    for i from 0 to 10 {
        arr[i] = i;
    }

    int[] arr2 = { 10, 20 };

    arr[1] = arr2[1];
    throwIf(arr[1] != arr2[1]);

    arr[0] += 10;
    throwIf(arr[0] != 10);
    arr[0] -= 10;
    throwIf(arr[0] != 0);
    arr[0] = 1;
    arr[0] *= 10;
    throwIf(arr[0] != 10);
    arr[0] /= 10;
    throwIf(arr[0] != 1);
    arr[0] %= 1;
    throwIf(arr[0] != 0);
}
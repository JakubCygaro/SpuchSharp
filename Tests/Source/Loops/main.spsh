import "STDLib";

fun main() {
    # while
    int i;
    while(i < 10) {
        i++;
    }
    throwIf(i != 10);

    # for
    for n from 0 to 10 {
        i = n;
    }
    throwIf(i != 9);

    # loop
    i = 0;
    loop {
        i++;
        if(i >= 10) {
            break;
        }
    }
    throwIf(i != 10);


}

